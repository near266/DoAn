    "emoji-mart": "^3.0.1",
    "formik": "^2.2.9",
    "graphql": "^15.5.1",
    "lodash-es": "^4.17.21",
    "mustache": "^4.2.0",
    "next": "^12.3.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "quill": "^1.3.7",
