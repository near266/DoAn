    "lint-staged": "^12.3.7",
    "postcss": "^8.4.35",
    "prettier": "^2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.6.3"
