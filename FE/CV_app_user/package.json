{
  "name": "youth-plus",
  "version": "0.1.0",
  "repository": "https://github.com/vietanh74/youth-plus-app",
  "author": "vietanh <tavietanh74@gmail.com>",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3000",
    "start": "next start -p ${PORT-3000}",
    "types-check": "tsc --pretty --noEmit",
    "lint:prettier": "prettier --check \"src/**/*+(js|jsx|ts|tsx|scss)\"",
    "lint:script": "eslint -c .eslintrc.js \"./src/**/*.{ts,tsx}\"",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint": "npm run lint:script && npm run lint:prettier && npm run lint:style",
    "lintfix": "prettier --write --list-different \"src/**/*+(js|jsx|ts|tsx|scss)\" && npm run lint:script -- --fix && npm run lint:style -- --fix",
    "prepare": "husky install",
    "first": "NODE_ENV=production yarn build && pm2 start yarn  --interpreter bash --name youth -- start",
    "production": "NODE_ENV=production yarn build && pm2 delete youth && pm2 start yarn  --interpreter bash --name youth -- start"
  },
  "dependencies": {
